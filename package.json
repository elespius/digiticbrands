  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "env",
              {
                "targets": {
                  "browsers": [
                    "last 2 versions",
                    "IE 11"
                  ]
                }
              }
            ]
          ],
          "plugins": [
            "static-fs"
          ]
        }
      ]
    ]
  },
  "settings": {
    "browsers": ["last 1 version"],
    "webcomponents": {
      "field-media": {
        "css": "media/system/webcomponents/css",
        "js": "media/system/webcomponents/js"
      }
    },
    "vendors": {
      "awesomplete": {
